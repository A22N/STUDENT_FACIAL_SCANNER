<div class="page-header">
  <h3 class="page-title">Thiết bị quét mặt</h3>
</div>

<div class="card">
  <div class="card-body table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Tên thiết bị</th>
          <th>IP</th>
          <th>Trạng thái</th>
          <th>Lần cuối cập nhật</th>
        </tr>
      </thead>
      <tbody>
        <% devices.forEach(d => { %>
        <tr>
          <td><%= d.name %></td>
          <td><%= d.ip %></td>
          <td><%= d.status %></td>
          <td><%= d.last_seen %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
